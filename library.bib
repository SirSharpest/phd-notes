  keywords = {Arabidopsis,Plasmodesmata,cell-to-cell communication,Arabidopsis Proteins,Plant Diseases,Botrytis,Chitin,Aniline Compounds,Cell Communication,Electrophoretic Mobility Shift Assay,Microscopy; Confocal,PAMP-triggered immunity,Plants; Genetically Modified,Protein-Serine-Threonine Kinases,Reactive Oxygen Species,Receptors; Cell Surface,Respiratory Burst,Trypan Blue},
  pages = {9166-9170},
  file = {/Users/hughesn/Zotero/storage/G4JRWMHW/Faulkner et al. - 2013 - LYM2-dependent chitin perception limits molecular .pdf},

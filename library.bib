  month = sep,
  year = {2018},
  pages = {1-26},
  file = {/Users/nathan/Google Drive/PHD/Zotfile/B/Brault et al. - 2018 - Multiple C2 domains and Transmembrane region Prote.pdf},
  note = {00000}
}


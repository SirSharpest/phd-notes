  author = {Stahl, Bethany A. and Gross, Joshua B. and Speiser, Daniel I. and Oakley, Todd H. and Patel, Nipam H. and Gould, Douglas B. and Protas, Meredith E.},
  year = {13-Oct-2015},
  keywords = {Eyes,Gene mapping,Gene ontologies,Genetic loci,Invertebrate genomics,Linkage mapping,Sequence assembly tools,Transcriptome analysis},
  pages = {e0140484},
  file = {/Users/nathan/Google Drive/PHD/Zotfile/S/Stahl et al. - 2015 - A Transcriptomic Analysis of Cave, Surface, and Hy.pdf;/Users/nathan/Zotero/storage/H4VQGLCL/article.html},

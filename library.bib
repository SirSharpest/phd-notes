  author = {Daryanto, Stefani and Wang, Lixin and Jacinthe, Pierre Andr{\'e}},
  year = {2016},
  pages = {1-15},
  file = {/Users/nathan/Google Drive/PHD/Zotfile/D/Daryanto et al. - 2016 - Global synthesis of drought effects on maize and w.pdf},
  isbn = {1932-6203},
  pmid = {26061704}
}

@article{Stahl2013,

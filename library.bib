  keywords = {Analytic reconstruction,Artifact reduction,Dual-source CT,Energy-sensitive CT,Flat-panel based CT,Iterative reconstruction,Local/interior reconstruction,Modality fusion,Multi-source CT,Nano-CT,Phase-contrast CT,Scanning mode},
  pages = {1051-1064},
  pmid = {18404940},
  note = {00270}
}

@article{zipfelPlantPatternrecognitionReceptors2014,
  title = {Plant {{Pattern}}-{{Recognition Receptors}}},
  volume = {35},
  issn = {1471-4906},
  doi = {10.1016/J.IT.2014.05.004},
  abstract = {Plants are constantly exposed to would-be pathogens in their immediate environment. Yet, despite relying on innate immunity only, plants are resistant to most microbes. They employ pattern-recognition receptors (PRRs) for sensitive and rapid detection of the potential danger caused by microbes and pests. Plant PRRs are either surface-localized receptor kinases (RKs) or receptor-like proteins (RLPs) containing various ligand-binding ectodomains that perceive pathogen-associated molecular patterns (PAMPs) or damage-associated molecular patterns (DAMPs). In this review, I summarize our current knowledge of plant PRRs and their ligands, illustrating the multiple molecular strategies employed by plant PRRs to activate innate immune signaling to survive.},
  number = {7},
  journal = {Trends in Immunology},
  author = {Zipfel, Cyril},
  month = jul,
  year = {2014},
  pages = {345-351},
  note = {00377}
}

@article{charmetWheatDomesticationLessons2011,
  title = {Wheat {{Domestication}}: {{Lessons}} for the {{Future}}},
  volume = {334},
  issn = {16310691},
  doi = {10.1016/j.crvi.2010.12.013},
  abstract = {Wheat was one of the first crops to be domesticated more than 10,000 years ago in the Middle East. Molecular genetics and archaeological data have allowed the reconstruction of plausible domestication scenarios leading to modern cultivars. For diploid einkorn and tetraploid durum wheat, a single domestication event has likely occurred in the Karacadag Mountains, Turkey. Following a cross between tetraploid durum and diploid T. tauschii, the resultant hexaploid bread wheat was domesticated and disseminated around the Caucasian region. These polyploidisation events facilitated wheat domestication and created genetic bottlenecks, which excluded potentially adaptive alleles. With the urgent need to accelerate genetic progress to confront the challenges of climate change and sustainable agriculture, wild ancestors and old landraces represent a reservoir of underexploited genetic diversity that may be utilized through modern breeding methods. Understanding domestication processes may thus help identifying new strategies. \textcopyright{} 2011 Acad\'emie des sciences.},
  number = {3},
  journal = {Comptes Rendus - Biologies},
  author = {Charmet, Gilles},
  month = mar,
  year = {2011},
  keywords = {Adaptation,Breeding,Polyploidisation,T. aestivum,T. turgidum,Triticum monococcum},
  pages = {212-220},
  pmid = {21377616},
  note = {00068}
}

@article{warMechanismsPlantDefense2012,
  title = {Mechanisms of {{Plant Defense}} against {{Insect Herbivores}}},

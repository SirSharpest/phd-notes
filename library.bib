  journal = {Molecular Breeding},
  author = {Peng, Junhua H. and Sun, Dongfa and Nevo, Eviatar},
  month = oct,
  year = {2011},
  keywords = {Cultivated wheat,Domestication syndrome factor,Domestication-related QTL,Evolution and domestication,Major domestication gene,Wild emmer wheat},

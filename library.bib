  isbn = {1573-4978 (Electronic)\textbackslash{}n0301-4851 (Linking)},
  mendeley-tags = {Arabidopsis thaliana,Botrytis cinerea,Defence,Grey mould,Microarray,Necrotroph},
  pmid = {21785916},

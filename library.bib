  month = jan,
  year = {2018},
  keywords = {cell-to-cell communication,effectors,microbe-associated molecular patterns (MAMPs),plant immunity,plasmodesmata (PD),systemic responses},
  pages = {62-67},
  isbn = {1469-8137 (Electronic) 0028-646X (Linking)},
  pmid = {29083038}
}

